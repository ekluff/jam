              </div>
              <div class="input-field col s12 m6">
                <i class="material-icons prefix">vpn_key</i>
                <input id="icon-prefix" name="password_confirmation" type="password" placeholder="">
                <label for="icon-prefix">Password Confirmation</label>
              </div>
            </div>
        </div>
      </div>
    </div>
  </div>

